  "devDependencies": {
    "react-test-renderer": "^18.0.0",
    "redux-mock-store": "^1.5.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.4",
    "prettier": "2.6.2"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.1",
    "history": "^5.3.0",
